{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "USE_LOCAL_DATA": "true",
    "DASH_DEBUG": "false",
    "USE_SIMPLE_CHURN": "true",
    "DATA_PATH": "./data/raw/"
  }
}
